{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node", "@cloudflare/workers-types"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "NodeNext"
  },
  "exclude": ["node_modules"]
}
